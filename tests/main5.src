struct BASE {
    public let a: float;
	// private let b: float;
};

struct DERIVED inherits BASE {
    // private let c: float;
    private func test() -> void;
    public func someFunction(x: BASE) -> float;
};

impl DERIVED {
    func test() -> void {}
    func someFunction(x: BASE) -> float {
        test();
        let a: integer;
        a = 0;
        a = a + 10;
        return (x.a + a);
    }
}

func main() -> void {
    let result: DERIVED[3];
    let test: BASE;
    result[0].someFunction(test);
    // result[4].test();
