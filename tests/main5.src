struct BASE {
    private let a: float;
	private let b: float;
};

struct DERIVED inherits BASE {
    private let c: float;
    public func someFunction(x: BASE) -> float;
};

impl DERIVED {
    func someFunction(x: BASE) -> float
    {
        return (x + 10.0 * 33);
    }
}

func main() -> void {
    let result: DERIVED;
    result.a = 10;
    result.b = 20;
    result.someFunction(3.0);
}
