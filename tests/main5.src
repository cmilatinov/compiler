struct BASE {
    public let a: integer;
	public let b: integer;
	public func test() -> void;
	public func someFunction(x: BASE) -> float;
};

struct DERIVED inherits BASE {
    public let c: integer;
    public func someFunction(x: BASE) -> float;
};

impl BASE {
    func test() -> void {}
    func someFunction(x: BASE) -> float {
        return (x.a);
    }
}

impl DERIVED {
    func someFunction(x: BASE) -> float {
        test();
        let b: integer;
        b = 0;
        b = b + 10;
        return (x.a + c * b / a);
    }
}

func main() -> void {
    let result: DERIVED[3];
    let test: BASE;
    let abc: integer;
    abc = 10;
    result[0].someFunction(test);
    write(result[0].a);
    read(abc);
    return (result[abc * 3].test());
}
