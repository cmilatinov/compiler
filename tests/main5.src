	public func someFunction(x: BASE) -> float;
};

struct DERIVED inherits BASE {
    public let a: float;
    public let c: float;
    public func test() -> void;
    public func someFunction(x: BASE) -> float;
};

impl BASE {
    func test() -> void {}
    func someFunction(x: BASE) -> float {
        return (x.a);
    }
}

impl DERIVED {
    func test() -> void {}
    func someFunction(x: BASE) -> float {
        test();
        let b: integer;
        b = 0;
        b = b + 10;
        return (x.a + c);
    }
}

func main() -> void {
    let result: DERIVED[3];
    let test: BASE;
    result[0].someFunction(test);
    result[4].test();
